<% content_for(:wf_page, "5ddbe39b578bab30c22dbad5") %>

<%
  if data.pages.home.seo && data.pages.home.seo.title
    content_for(:page_title, data.pages.home.seo.title)
  end
%>
<%
  if data.pages.home.seo && data.pages.home.seo.description
    content_for(:meta_description, data.pages.home.seo.description)
  end
%>

  <div data-w-id="3f4fd706-80ff-1556-716c-cb74667440c9" class="site-content">
    <div data-autoplay="true" data-loop="true" data-wf-ignore="true" data-poster-url="videos/bg-ck_1-poster-00001.jpg" data-video-urls="<%= data.pages.home.hero.video %>" class="hero-section_bg-video w-background-video w-background-video-atom"><video autoplay="" loop="" style="background-image:url(&quot;videos/bg-ck_1-poster-00001.jpg&quot;)" muted="" playsinline="" data-wf-ignore="true" data-object-fit="cover"><source src="<%= data.pages.home.hero.video %>" data-wf-ignore="true"></video>
      <%= partial "partials/navbar" %>

      <div class="hero-section_content">
        <div class="container variable">
          <div class="hero-section_content_title-box">
            <h1 class="hero-section_content_h1">Escritório de arquitetura em Gramado</h1>
            <div class="hero-section_content_title-box_main"><%= markdown(data.pages.home.hero.title) %></div>

            <div class="mobile-letter-change">
              <div data-w-id="81ea6d77-b84a-3bb5-f445-599bb4bbc8a6" class="mobile-letter-change_1"><%= data.pages.home.hero.subtitle %></div>
              <div data-w-id="757dbdad-22e4-1c77-ddca-8fc43181e254" class="mobile-letter-change_2"><%= data.pages.home.hero.subtitle2 %></div>
            </div><a href="<%= data.pages.home.hero.cta_link %>" class="button outline w-button"><%= data.pages.home.hero.cta_text %></a></div>
        </div>
        <div class="hero-section_content_scroll-box">
          <div class="hero-section_content_scroll-box_content"><img src="/assets/images/scroll-mouse.svg" width="15" alt="" class="hero-section_content_scroll-box_content_mouse">
            <div class="hero-section_content_scroll-box_content_arrow-box"><img src="/assets/images/scroll-arrow.svg" width="10" data-w-id="57084dfa-e7b5-509a-4141-977b26deb2d9" alt="" class="hero-section_content_scroll-box_content_arrow"></div>
          </div>
        </div>
      </div>
    </div>

    <% if empreendimento %>
      <div class="slider-section">
        <div class="container full-mobile" data-ix="scrollfadein">
          <div class="slider-section_title"><%= empreendimento.title %></div>
          <div data-delay="4000" data-animation="cross" data-autoplay="1" data-duration="700" data-infinite="1" class="slider-section_slider w-slider" data-ix="scrollfadein">
            <div class="slider-section_slider_mask w-slider-mask">
              <% empreendimento.highlight_images.each do |highlight| %>
                <div class="slider-section_slider_slide w-slide">
                  <div class="lazyload slide-item" data-bg="<%= highlight.image %>"></div>
                </div>
              <% end %>
            </div>
            <div class="left-arrow w-slider-arrow-left">
              <div class="w-icon-slider-left"></div>
            </div>
            <div class="right-arrow w-slider-arrow-right">
              <div class="w-icon-slider-right"></div>
            </div>
            <div class="slider-section_nav w-slider-nav w-slider-nav-invert w-round"></div>
          </div>
        </div>
        <div class="container" data-ix="scrollfadein">
          <div class="slider-section_content" data-ix="scrollfadein">
            <h2 class="slider-section_content_h2"><%= markdown(empreendimento.highlight_text) %></h2>
            <a href="/empreendimento/<%= empreendimento.title.parameterize %>" class="button outline w-button">Conheça a <%= empreendimento.title %></a></div>
        </div>
      </div>
    <% end %>

    <div id="houses-section" class="houses-section" data-ix="scrollfadein">
      <div class="container">
        <h2 class="houses-section_h2"><%= markdown(data.pages.home.title_empreendimentos) %></h2>
        <div class="houses-section_content">
          <% data.settings.navemp.items.each do |item| %>
            <% empreendimento = data.empreendimentos.find do |_filename, empreendimento| %>
              <% item.empreendimento.parameterize == empreendimento.title.parameterize %>
            <% end[1] %>
            <% condominio = data.condominios.find {|_filename, condominio| empreendimento.condominio == condominio.title} %>
            <a href="/empreendimento/<%= empreendimento.title.parameterize %>" class="houses-section_content_block_item w-inline-block">
              <div class="lazyload houses-section_content_block_item_image" data-bg="<%= empreendimento.image %>"></div>
              <h3 class="houses-section_content_block_item_h3"><%= empreendimento.title %></h3>
              <% if condominio %>
                <div class="houses-section_content_block_item_address"><%= condominio[1].title %></div>
              <% end %>
              <div class="houses-section_content_block_item_city"><%= empreendimento.city %></div>
            </a>
          <% end %>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="houses-section_scroll">
        <div class="houses-section_scroll_arrows"><a href="#" class="houses-section_scroll_arrows_item left disabled w-inline-block"><img src="/assets/images/slider-arrow.svg" width="35" alt=""></a><a href="#" class="houses-section_scroll_arrows_item w-inline-block"><img src="/assets/images/slider-arrow.svg" width="35" alt=""></a></div>
      </div>
    </div>
    <div class="sustainability_section" data-ix="scrollfadein">
      <div class="container">
        <div class="sustainability_section_intro">
          <div class="sustainability_section_intro_left">
            <h2 class="sustainability_section_intro_left_h2"><%= markdown(data.pages.home.sustentabilidade.title) %></h2><img src="/assets/images/Alma-verde3.png" width="140" srcset="/assets/images/Alma-verde3-p-500.png 500w, images/Alma-verde3-p-800.png 800w, images/Alma-verde3-p-1080.png 1080w, images/Alma-verde3.png 1490w" sizes="140px" alt="" class="sustainability_section_intro_left_logo"></div>
          <div class="sustainability_section_intro_right">
            <div class="sustainability_section_intro_right_paragraph"><%= markdown(data.pages.home.sustentabilidade.text) %></div>
          </div>
        </div>
        <div class="sustainability_section_gallery">
          <% data.pages.home.sustentabilidade.items.each do |item| %>
            <a href="#" class="sustainability_section_gallery_item <%= item.bottom ? 'bottom ' : '' %>w-inline-block">
              <div data-w-id="98211e8d-26a5-2ec5-4d02-7a6b0ef1980b" class="sustainability_section_gallery_item_block" style="background-image: linear-gradient(180deg, rgba(0, 0, 0, 0.45), rgba(0, 0, 0, 0.45)), url('<%= item.image %>')">
                <img data-src="<%= item.external_icon %>" width="65" alt="" class="lazyload sustainability_section_gallery_item_block_icon">
              </div>
              <h3 class="sustainability_section_gallery_item_h3"><%= item.title %></h3>
              <div style="display:none;opacity:0" class="sustainability_section_gallery_item_modal">
                <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)" class="sustainability_section_gallery_item_modal_box">
                  <div class="sustainability_section_gallery_item_modal_box_header">
                    <img data-src="<%= item.internal_icon %>" width="40" alt="" class="lazyload sustainability_section_gallery_item_modal_box_header_icon">
                    <div data-w-id="545c32ca-a8ef-ef46-b05b-7297f883ca1d" class="sustainability_section_gallery_item_modal_box_header_close">
                      <img src="/assets/images/modal-close_grey.svg" width="15" alt="">
                    </div>
                  </div>
                  <h3 class="sustainability_section_gallery_item_modal_box_h3"><%= item.complete_title %></h3>
                  <p><%= item.text %></p>
                  <img src="/assets/images/ck-simbolo.svg" width="35" alt="" class="sustainability_section_gallery_item_modal_logo">
                </div>
              </div>
            </a>
          <% end %>
        </div>
      </div>
    </div>
    <div class="container">
      <h2 class="news-section_h2">CK arquitetura<br><strong class="bold-pink">na mídia</strong></h2>
    </div>
    <div id="houses-section" class="houses-section" data-ix="scrollfadein">
      <div class="container">
        <div class="houses-section_content">
          <% data.pages.home.midia.items.each do |item| %>
            <a href="<%= item.link %>" target="_blank" class="news-section_list_item w-inline-block">
              <div class="lazyload news-section_item" data-bg="<%= item.image %>"></div>
              <h3 class="news-section_item_h3"><%= item.title %></h3>
            </a>
          <% end %>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="houses-section_scroll">
        <div class="houses-section_scroll_arrows"><a href="#" class="houses-section_scroll_arrows_item left disabled w-inline-block"><img src="/assets/images/slider-arrow.svg" width="35" alt=""></a><a href="#" class="houses-section_scroll_arrows_item w-inline-block"><img src="/assets/images/slider-arrow.svg" width="35" alt=""></a></div>
      </div>
    </div>
    <%= partial "partials/footer" %>
  </div>