                    <img src="<%= item.internal_icon %>" width="40" alt="" class="sustainability_section_gallery_item_modal_box_header_icon">
                    <div data-w-id="545c32ca-a8ef-ef46-b05b-7297f883ca1d" class="sustainability_section_gallery_item_modal_box_header_close">
                      <img src="images/modal-close_grey.svg" width="15" alt="">
                    </div>
                  </div>
                  <h3 class="sustainability_section_gallery_item_modal_box_h3"><%= item.complete_title %></h3>
                  <p><%= item.text %></p>
                </div>
              </div>
            </a>
          <% end %>
        </div>
      </div>
    </div>
