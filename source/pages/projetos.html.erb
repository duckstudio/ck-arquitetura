<html data-wf-page="5e2f88b29cd39125245408dc" data-wf-site="5ddbe39b578bab133d2dbad4">
<head>
  <meta charset="utf-8">
  <title>Projetos - CK Arquitetura</title>
  <meta content="A CK Arquitetura é uma jovem e arrojada empresa incorporadora da Cidade de  Gramado/RS, dirigida pela paixão, integridade e amor a arquitetura e ao bem viver." name="description">
  <meta content="Projetos - CK Arquitetura" property="og:title">
  <meta content="A CK Arquitetura é uma jovem e arrojada empresa incorporadora da Cidade de  Gramado/RS, dirigida pela paixão, integridade e amor a arquitetura e ao bem viver." property="og:description">
  <meta content="https://i.ibb.co/QJSwHBW/ogImage.png" property="og:image">
  <meta content="summary" name="twitter:card">
  <meta content="width=device-width, initial-scale=1" name="viewport">
  <link href="/css/normalize.css" rel="stylesheet" type="text/css">
  <link href="/css/components.css" rel="stylesheet" type="text/css">
  <link href="/css/ck-arquitetura.css" rel="stylesheet" type="text/css">
  <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
  <script type="text/javascript">
      WebFont.load({
        google: {
          families: ["Poppins:100,100italic,200,200italic,300,300italic,regular,italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic"]
        }
      });
    </script>
  <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
  <script type="text/javascript">
      ! function(o, c) {
        var n = c.documentElement,
          t = " w-mod-";
        n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch")
      }(window, document);
    </script>
  <link href="/images/favicon.png" rel="shortcut icon" type="image/x-icon">
  <link href="/images/webclip.png" rel="apple-touch-icon">
  <style>
      .w-slider-dot {
        margin: 0 3px 1.5em !important;
        background-color: transparent !important;
        border: solid 1px #a07987 !important;
      }
      .w-slider-nav-invert>div.w-active {
        background-color: #a07987 !important;
      }
      #houses-section::-webkit-scrollbar {
        display: none;
      }
      .houses-section {
        overflow-y: hidden !important;
      }
    </style>
</head>
<body class="body">
  <%= partial "partials/navbar", locals: { white: true } %>

  <div class="about-page_intro-section">
    <div class="container">
      <div class="broker-page_intro-section_content">
        <div>
          <h1 class="about-page_intro-section_h1"><strong><%= markdown data.pages["pagina-projetos"].title %></strong></h1>
          <div class="about-page_intro-section_subtitle"><%= data.pages["pagina-projetos"].subtitle %></div>
        </div>
        <div class="broker-page_intro-section_content_block">
          <p class="broker-page_intro-section_content_block_p projects-page"><%= data.pages["pagina-projetos"].text %></p>
        </div>
      </div>
    </div>
  </div>
  <div class="projects-page_list">
    <% data.projetos.each_slice(6) do |project_slice| %>
      <a href="/projeto/<%= project_slice[0][1].title.parameterize %>" class="projects-page_list_item w-inline-block" style="background-image: url('<%= project_slice[0][1].image %>')">
        <div class="projects-page_list_item_txt">
          <div class="projects-page_list_item_txt_name"><%= project_slice[0][1].title %></div>
          <div class="projects-page_list_item_txt_divisor"></div>
          <div class="projects-page_list_item_txt_city"><%= project_slice[0][1].city %></div>
        </div>
      </a>
      <% if project_slice[1] %>
        <div class="projects-page_list_row">
          <a href="/projeto/<%= project_slice[1][1].title.parameterize %>" class="projects-page_list_item w-inline-block">
            <div class="projects-page_list_item_txt">
              <div class="projects-page_list_item_txt_name"><%= project_slice[1][1].title %></div>
              <div class="projects-page_list_item_txt_divisor"></div>
              <div class="projects-page_list_item_txt_city"><%= project_slice[1][1].city %></div>
            </div>
          </a>
          <% if project_slice[2] %>
            <a href="/projeto/<%= project_slice[2][1].title.parameterize %>" class="projects-page_list_item w-inline-block">
              <div class="projects-page_list_item_txt">
                <div class="projects-page_list_item_txt_name"><%= project_slice[2][1].title %></div>
                <div class="projects-page_list_item_txt_divisor"></div>
                <div class="projects-page_list_item_txt_city"><%= project_slice[2][1].city %></div>
              </div>
            </a>
          <% end %>
        </div>
        <% if project_slice[3] %>
          <div class="projects-page_list_row">
            <div class="projects-page_list_row_vertical">
              <a href="/projeto/<%= project_slice[3][1].title.parameterize %>" class="projects-page_list_item w-inline-block">
                <div class="projects-page_list_item_txt">
                  <div class="projects-page_list_item_txt_name"><%= project_slice[3][1].title %></div>
                  <div class="projects-page_list_item_txt_divisor"></div>
                  <div class="projects-page_list_item_txt_city"><%= project_slice[3][1].city %></div>
                </div>
              </a>
              <% if project_slice[4] %>
                <a href="/projeto/<%= project_slice[4][1].title.parameterize %>" class="projects-page_list_item w-inline-block">
                  <div class="projects-page_list_item_txt">
                    <div class="projects-page_list_item_txt_name"><%= project_slice[4][1].title %></div>
                    <div class="projects-page_list_item_txt_divisor"></div>
                    <div class="projects-page_list_item_txt_city"><%= project_slice[4][1].city %></div>
                  </div>
                </a>
              <% end %>
            </div>
            <% if project_slice[5] %>
              <a href="/projeto/<%= project_slice[5][1].title.parameterize %>" class="projects-page_list_item w-inline-block">
                <div class="projects-page_list_item_txt">
                  <div class="projects-page_list_item_txt_name"><%= project_slice[5][1].title %></div>
                  <div class="projects-page_list_item_txt_divisor"></div>
                  <div class="projects-page_list_item_txt_city"><%= project_slice[5][1].city %></div>
                </div>
              </a>
            <% end %>
          </div>
        <% end %>
      <% end %>
    <% end %>
  </div>
  <div class="projects-page_testimonial-section">
    <div class="container">
      <h2 class="projects-page_testimonial-section_h2"><%= data.pages["pagina-projetos"].title_testimonials %></h2>
      <div data-animation="slide" data-duration="500" data-infinite="1" class="projects-page_testimonial-section_slider w-slider">
        <div class="projects-page_testimonial-section_slider_mask w-slider-mask">
          <% data.pages["pagina-projetos"].items.each_slice(2) do |item_slice| %>
            <div class="projects-page_testimonial-section_slider_item w-slide">
              <div class="projects-page_testimonial-section_content">
                <div class="projects-page_testimonial-section_content_item">
                  <p class="projects-page_testimonial-section_content_item_p"><%= item_slice[0].text %></p>
                  <div class="projects-page_testimonial-section_content_item_line"></div>
                  <div class="projects-page_testimonial-section_content_item_details">
                    <div class="projects-page_testimonial-section_content_item_details_photo" style="background-image: url('<%= item_slice[0].image %>')"></div>
                    <div class="projects-page_testimonial-section_content_item_details_name"><%= item_slice[0].title %></div>
                  </div>
                </div>
                <% if item_slice[1] %>
                  <div class="projects-page_testimonial-section_content_item">
                    <p class="projects-page_testimonial-section_content_item_p"><%= item_slice[1].text %></p>
                    <div class="projects-page_testimonial-section_content_item_line"></div>
                    <div class="projects-page_testimonial-section_content_item_details">
                      <div class="projects-page_testimonial-section_content_item_details_photo" style="background-image: url('<%= item_slice[1].image %>')"></div>
                      <div class="projects-page_testimonial-section_content_item_details_name"><%= item_slice[1].title %></div>
                    </div>
                  </div>
                <% end %>
              </div>
            </div>
          <% end %>
        </div>
        <div class="projects-page_testimonial-section_slider_arrow w-slider-arrow-left">
          <div class="w-icon-slider-left"></div>
        </div>
        <div class="projects-page_testimonial-section_slider_arrow w-slider-arrow-right">
          <div class="w-icon-slider-right"></div>
        </div>
        <div class="projects-page_testimonial-section_slider_nav w-slider-nav w-slider-nav-invert w-round"></div>
      </div>
    </div>
  </div>
  <div class="house-page_contact-section">
    <div class="container">
      <h2 class="house-page_contact-section_h2">Vamos <strong class="semi-bold">conversar?</strong></h2>
      <div class="house-page_contact-section_subtitle">Fale diretamente com nossos consultores, a qualquer momento.</div>
      <div class="house-page_contact-section_buttons-box"><a href="https://api.whatsapp.com/send?phone=5554997514953&amp;text=Ol%C3%A1,%20estou%20entrando%20em%20contato%20pelo%20site!" target="_blank" class="button pink w-button">Enviar mensagem pelo WhatsApp</a><a href="mailto:comercial@ckarquitetura.com?subject=Contato%20Site" class="button pink fill margin-left w-button">Entrar em contato pelo e-mail</a></div>
    </div>
  </div>

  <%= partial "partials/footer" %>
  <%= partial "partials/menu-mobile" %>

  <%= partial "partials/scripts" %>
</body>
</html>
