<html data-wf-page="5e388b43ad9b09809eeae504" data-wf-site="5ddbe39b578bab133d2dbad4">
<head>
  <meta charset="utf-8">
  <title>Empreendimentos</title>
  <meta content="A CK Arquitetura é uma jovem e arrojada empresa incorporadora da Cidade de  Gramado/RS, dirigida pela paixão, integridade e amor a arquitetura e ao bem viver." name="description">
  <meta content="Empreendimentos" property="og:title">
  <meta content="A CK Arquitetura é uma jovem e arrojada empresa incorporadora da Cidade de  Gramado/RS, dirigida pela paixão, integridade e amor a arquitetura e ao bem viver." property="og:description">
  <meta content="https://i.ibb.co/QJSwHBW/ogImage.png" property="og:image">
  <meta content="summary" name="twitter:card">
  <meta content="width=device-width, initial-scale=1" name="viewport">
  <link href="/css/normalize.css" rel="stylesheet" type="text/css">
  <link href="/css/components.css" rel="stylesheet" type="text/css">
  <link href="/css/ck-arquitetura.css" rel="stylesheet" type="text/css">
  <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
  <script type="text/javascript">
      WebFont.load({
        google: {
          families: ["Poppins:100,100italic,200,200italic,300,300italic,regular,italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic"]
        }
      });
    </script>
  <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
  <script type="text/javascript">
      ! function(o, c) {
        var n = c.documentElement,
          t = " w-mod-";
        n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch")
      }(window, document);
    </script>
  <link href="images/favicon.png" rel="shortcut icon" type="image/x-icon">
  <link href="images/webclip.png" rel="apple-touch-icon">
  <style>
      .w-slider-dot {
        margin: 0 3px 1.5em !important;
        background-color: transparent !important;
        border: solid 1px #a07987 !important;
      }
      .w-slider-nav-invert>div.w-active {
        background-color: #a07987 !important;
      }
      #houses-section::-webkit-scrollbar {
        display: none;
      }
      .houses-section {
        overflow-y: hidden !important;
      }
    </style>
</head>
<body class="body">
  <div style="display:none;opacity:0" class="download-modal">
    <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)" class="download-modal_box">
      <div class="download-modal_box_header">
        <div data-w-id="455dcea4-d4d0-27a4-0e9c-4060522a1292" class="sustainability_section_gallery_item_modal_box_header_close"><img src="images/modal-close_grey.svg" width="15" alt=""></div>
      </div>
      <h2 class="download-modal_h2">baixe o material completo deste empreendimento</h2>
      <div class="form-block w-form">
        <form id="email-form" name="email-form" data-name="Email Form" class="form-block_form">
          <div class="form-block_form_item"><label for="name-3" class="form-block_form_item_label">Nome completo</label><input type="text" class="form-block_form_item_input w-input" maxlength="256" name="name" data-name="name" placeholder="Digite aqui seu nome" id="name-3" required=""></div>
          <div class="form-block_form_item"><label for="email" class="form-block_form_item_label">E-mail profissional</label><input type="email" class="form-block_form_item_input w-input" maxlength="256" name="email" data-name="email" placeholder="Digite aqui o seu e-mail" id="email" required=""></div>
          <div class="form-block_row">
            <div class="form-block_form_item"><label for="creci" class="form-block_form_item_label">creci</label><input type="text" class="form-block_form_item_input w-input" maxlength="256" name="creci" data-name="creci" placeholder="Digite o seu CRECI" id="creci" required=""></div>
            <div class="form-block_form_item"><label for="phone" class="form-block_form_item_label">Telefone</label><input type="tel" class="form-block_form_item_input w-input" maxlength="256" name="phone" data-name="phone" placeholder="Digite o seu telefone" id="phone" required=""></div>
          </div>
          <div class="download-button_align"><a href="#" class="button pink w-button">Baixar</a></div>
        </form>
        <div class="w-form-done">
          <div>Thank you! Your submission has been received!</div>
        </div>
        <div class="w-form-fail">
          <div>Oops! Something went wrong while submitting the form.</div>
        </div>
      </div>
    </div>
  </div>

  <%= partial "partials/navbar", locals: { white: true } %>

  <div id="housesSection" class="broker-page_houses-section">
    <div class="container">
      <div class="broker-page_houses-section_list">
        <% data.empreendimentos.each do |_filename, empreendimento| %>
          <div class="houses-section_content_block_item broker-page">
            <div class="houses-section_content_block_item_image list" style="background-image: url('<%= empreendimento.image %>')"></div>
            <h3 class="houses-section_content_block_item_h3"><%= empreendimento.title %></h3>
            <div class="houses-section_content_block_item_address">Aspen Mountain</div>
            <div class="houses-section_content_block_item_city"><%= empreendimento.city %></div>
            <a data-w-id="6a0f4f2d-897d-206f-d18d-ac0e1b7e6f4b" href="/empreendimento/<%= empreendimento.title.parameterize %>" class="button w-button">Ver mais detalhes</a>
          </div>
        <% end %>
      </div>
    </div>
  </div>

  <%= partial "partials/footer" %>
  <%= partial "partials/menu-mobile" %>

  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
  <script src="/js/ck-arquitetura.js" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
  <!-- Start of HubSpot Embed Code -->
    <script type="text/javascript" id="hs-script-loader" async defer src="//js.hs-scripts.com/7289857.js"></script>
  <!-- End of HubSpot Embed Code -->
</body>
</html>
