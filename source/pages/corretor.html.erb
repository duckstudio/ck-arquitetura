        <h1 class="about-page_intro-section_h1"><%= markdown data.pages.corretor.title %></h1>
        <div class="broker-page_intro-section_content_block">
          <div class="broker-page_intro-section_content_block_p"><%= markdown data.pages.corretor.text %></div>
          <a href="#housesSection" class="button outline w-button"><%= data.pages.corretor.button %></a>
        </div>
      </div>
    </div>
  </div>
  <div class="broker-page_video-section">
    <div data-autoplay="true" data-loop="true" data-wf-ignore="true" data-poster-url="videos/bg-ck_1-poster-00001.jpg" data-video-urls="<%= data.pages.corretor.background_video %>" class="broker-page_video-section_video w-background-video w-background-video-atom">
