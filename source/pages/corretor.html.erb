        <h2 class="broker-page_bonus-section1_content_h2"><%= markdown data.pages.corretor.title_bonus1 %></h2>
        <p class="broker-page_bonus-section1_content_p"><%= data.pages.corretor.text_bonus1 %></p>
      </div>
    </div>
  </div>
  <div class="broker-page_bonus-section2">
    <div class="container">
      <img src="<%= data.pages.corretor.image_bonus2 %>" width="386" alt="" class="broker-page_bonus-section2_image mobile">
      <div class="broker-page_bonus-section2_intro">
        <h2 class="broker-page_bonus-section1_content_h2 white"><%= markdown data.pages.corretor.title_bonus2 %></h2>
        <p class="broker-page_bonus-section1_content_p white"><%= data.pages.corretor.text_bonus2 %></p>
      </div>
      <div class="slider-section_content">
        <h2 class="slider-section_content_h2"><%= markdown data.pages.corretor.call %></h2>
        <a href="#housesSection" class="button outline w-button"><%= data.pages.corretor.button2 %></a>
      </div>
      <img src="<%= data.pages.corretor.image_bonus2 %>" width="386" alt="" class="broker-page_bonus-section2_image">
    </div>
  </div>
  <div id="housesSection" class="broker-page_houses-section">
    <div class="container">
      <div class="broker-page_houses-section_list">
        <% data.empreendimentos.each do |_filename, empreendimento| %>
          <% if empreendimento.file %>
            <div class="houses-section_content_block_item broker-page">
              <div class="houses-section_content_block_item_image" style="background-image: url('<%= empreendimento.image %>')"></div>
              <h3 class="houses-section_content_block_item_h3"><%= empreendimento.title %></h3>
              <div class="houses-section_content_block_item_address broker-page_house-subtitle"><%= empreendimento.subtitle %></div>
              <a
                data-w-id="6a0f4f2d-897d-206f-d18d-ac0e1b7e6f4b"
                href="#"
                class="button w-button"
                id="download-button"
                data-empreendimento="<%= empreendimento.title.parameterize %>"
              >
                Baixar material
              </a>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
  <div class="house-page_reasons-section broker">
    <div class="container">
      <h2 class="house-page_reasons-section_h2"><%= markdown data.pages.corretor.title_reasons %></h2>
      <div class="house-page_reasons-section_list">
        <% data.pages.corretor.items.each_with_index do |item, index| %>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header">
              <img src="<%= item.icon %>" width="60" alt="" class="house-page_reasons-section_list_item_header_icon">
            </div>
            <h3 class="house-page_reasons-section_list_item_h3">0<%= index + 1 %>. <strong><%= item.title %></strong></h3>
            <p class="house-page_reasons-section_list_item_p"><%= item.text %></p>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <%= partial "partials/footer" %>
  <%= partial "partials/menu-mobile" %>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
  <script src="/js/ck-arquitetura.js" type="text/javascript"></script>

  <script>
    const emp_files = {
      <% data.empreendimentos.each do |_filename, empreendimento| %>
        <%= "'#{empreendimento.title.parameterize}': '#{empreendimento.file}'" if empreendimento.file %>
      <% end %>
    };

    const button = document.getElementById("download-button");
    const button2 = document.getElementById("download-button-2");
    const form = document.getElementById("email-form");

    button.addEventListener('click', function (event) {
      const empreendimento = event.target.dataset.empreendimento;

      button2.href = emp_files[empreendimento];
    });

    button2.addEventListener('click', function (event) {
      form.submit();
    });
  </script>

