<html data-wf-page="5e2f888f9cd39160f9540658" data-wf-site="5ddbe39b578bab133d2dbad4">
<head>
  <meta charset="utf-8">
  <title>Página Empreendimento - CK Arquitetura</title>
  <meta content="A CK Arquitetura é uma jovem e arrojada empresa incorporadora da Cidade de  Gramado/RS, dirigida pela paixão, integridade e amor a arquitetura e ao bem viver." name="description">
  <meta content="Página Empreendimento - CK Arquitetura" property="og:title">
  <meta content="A CK Arquitetura é uma jovem e arrojada empresa incorporadora da Cidade de  Gramado/RS, dirigida pela paixão, integridade e amor a arquitetura e ao bem viver." property="og:description">
  <meta content="https://i.ibb.co/QJSwHBW/ogImage.png" property="og:image">
  <meta content="summary" name="twitter:card">
  <meta content="width=device-width, initial-scale=1" name="viewport">
  <link href="/css/normalize.css" rel="stylesheet" type="text/css">
  <link href="/css/components.css" rel="stylesheet" type="text/css">
  <link href="/css/ck-arquitetura.css" rel="stylesheet" type="text/css">
  <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
  <script type="text/javascript">
      WebFont.load({
        google: {
          families: ["Poppins:100,100italic,200,200italic,300,300italic,regular,italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic"]
        }
      });
    </script>
  <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
  <script type="text/javascript">
      ! function(o, c) {
        var n = c.documentElement,
          t = " w-mod-";
        n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch")
      }(window, document);
    </script>
  <link href="/images/favicon.png" rel="shortcut icon" type="image/x-icon">
  <link href="/images/webclip.png" rel="apple-touch-icon">
  <style>
      .w-slider-dot {
        margin: 0 3px 1.5em !important;
        background-color: transparent !important;
        border: solid 1px #a07987 !important;
      }
      .w-slider-nav-invert>div.w-active {
        background-color: #a07987 !important;
      }
      #houses-section::-webkit-scrollbar {
        display: none;
      }
      .houses-section {
        overflow-y: hidden !important;
      }
    </style>
</head>
<body class="body">
  <div class="youtube-modal"><a data-w-id="4a27a4b3-9dbd-c70b-ced1-d5b152b897da" href="#" class="youtube-modal_close w-inline-block"><img src="/images/close-icon.svg" width="24" alt=""></a>
    <div style="padding-top:56.17021276595745%" id="w-node-d5b152b897dc-52b897d9" class="youtube-modal_video w-video w-embed"><iframe class="embedly-embed" src="https://cdn.embedly.com/widgets/media.html?src=https%3A%2F%2Fwww.youtube.com%2Fembed%2F<%= empreendimento.youtube_video %>&display_name=YouTube&url=https%3A%2F%2Fwww.youtube.com%2Fwatch%3Fv%3D<%= empreendimento.youtube_video %>&image=http%3A%2F%2Fi.ytimg.com%2Fvi%2F<%= empreendimento.youtube_video %>%2Fhqdefault.jpg&key=96f1f04c5f4143bcb0f2e68c87d65feb&type=text%2Fhtml&schema=youtube" scrolling="no" title="YouTube embed" frameborder="0" allow="autoplay; fullscreen" allowfullscreen="true"></iframe></div>
  </div>
  <div class="project-page_hero">
    <div class="house-page_hero_main">
      <div data-collapse="medium" data-animation="default" data-duration="400" class="navbar w-nav">
        <div class="navbar_content"><a href="/" class="navbar_content_brand w-nav-brand"><img src="/images/logo-ck-arquitetura.svg" width="175" alt=""></a>
          <nav role="navigation" class="navbar_menu w-nav-menu">
            <div data-hover="1" data-delay="0" class="w-dropdown">
              <div class="navbar_menu_link dropdown w-dropdown-toggle">
                <div>Empreendimentos</div><img src="/images/dropdown-arrow.svg" width="10" alt="" class="navbar_menu_link_dropdown_icon"></div>
              <nav class="navbar_menu_link_dropdown_list w-dropdown-list">
                <div class="navbar_menu_link_dropdown_list_left">
                  <div class="navbar_menu_link_dropdown_list_left_title">Empreendimentos</div>
                  <div class="navbar_menu_link_dropdown_list_menu"><a href="/pagina-empreendimento" class="navbar_menu_link_dropdown_list_menu_item w-dropdown-link w--current">Casa Serena</a><a href="/pagina-empreendimento" class="navbar_menu_link_dropdown_list_menu_item w-dropdown-link w--current">CASA DOCE VIDA</a><a href="/pagina-empreendimento" class="navbar_menu_link_dropdown_list_menu_item w-dropdown-link w--current">CASA PUNTA</a></div><a href="/projetos" class="navbar_menu_link_dropdown_list_link">Projetos Personalizados</a></div>
                <div class="navbar_menu_link_dropdown_photo"></div>
              </nav>
            </div><a href="/institucional" class="navbar_menu_link w-nav-link">A CK Arquitetura</a><a href="/corretor" class="navbar_menu_link w-nav-link">corretores</a><a href="/contato" class="navbar_menu_link last w-nav-link">Contato</a></nav><a data-w-id="dfb2ea1d-9c1a-9dcf-5e2c-87a2a5b47e06" href="#" class="navbar_menu_btn w-inline-block"><img src="/images/hamburger-icon.svg" width="38" alt=""></a></div>
      </div>
      <div class="container">
        <h1 class="house-page_hero_h1"><%= empreendimento.title %></h1>
        <div class="house-page_hero_subtitle"><%= empreendimento.subtitle %></div>
      </div>
    </div>
    <div class="house-page_hero_footer">
      <div class="container">
        <div class="house-page_hero_footer_content">
          <div class="house-page_hero_footer_content_txt">
            <div class="house-page_hero_footer_content_txt_status">Andamento da obra:</div>
            <div class="house-page_hero_footer_content_txt_status_type"><%= empreendimento.status %></div>
          </div><a href="https://api.whatsapp.com/send?phone=5555197514953&amp;text=Ol%C3%A1,%20estou%20entrando%20em%20contato%20pelo%20site!" target="_blank" class="button pink fill w-button">Enviar mensagem pelo Whatsapp</a></div>
      </div>
    </div>
  </div>
  <div class="house-page_about-section">
    <div class="container">
      <p class="house-page_about-section_p"><%= empreendimento.text %></p>
      <div class="house-page_about-section_block">
        <div class="house-page_about-section_block_left">
          <% empreendimento.details.each do |detail| %>
            <div class="house-page_about-section_link">
              <img src="/images/ck-simbolo.svg" width="20" alt="" class="house-page_about-section_link_icon">
              <div class="house-page_about-section_link_text"><%= detail.title %></div>
              <div class="house-page_about-section_link_line"></div>
            </div>
          <% end %>
        </div>
        <div class="house-page_about-section_block_photo video">
          <div data-poster-url="/videos/bg-ck_1-poster-00001.jpg" data-video-urls="<%= empreendimento.background_video %>" data-autoplay="true" data-loop="true" data-wf-ignore="true" class="house-page_about-section_block_video w-background-video w-background-video-atom">
            <video autoplay="" loop="" style="background-image:url('/videos/bg-ck_1-poster-00001.jpg')" muted="" playsinline="" data-wf-ignore="true" data-object-fit="cover">
              <source src="<%= empreendimento.background_video %>" data-wf-ignore="true">
            </video>
            <a data-w-id="f0fae00a-98cb-7df7-070b-c40283f7d484" href="#" class="broker-page_video-section_video_link w-inline-block">
              <img src="/images/play.svg" width="85" alt="">
              <div class="broker-page_video-section_video_link_text">Ver o vídeo completo</div>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% empreendimento.image_sections.each do |image_section| %>
    <div class="house-page_gallery-section">
      <div class="container">
        <h2 class="house-page_gallery-section_title"><%= image_section.title %></h2>
      </div>
      <% image_section.images.each_slice(3) do |image_slice| %>
        <div class="house-page_gallery-section_main-photo" style="background-image: url('<%= image_slice[0].image %>')"></div>
        <% if image_slice[1] %>
          <div class="house-page_gallery-section_row">
            <div class="house-page_gallery-section_row_photo" style="background-image: url('<%= image_slice[1].image %>')"></div>
            <% if image_slice[2] %>
              <div class="house-page_gallery-section_row_photo _2" style="background-image: url('<%= image_slice[2].image %>')"></div>
            <% end %>
          </div>
        <% end %>
      <% end %>
    </div>
  <% end %>
  <div class="house-page_map-section">
    <div class="container">
      <h2 class="house-page_map-section_h2">Plantas</h2>
      <% empreendimento.plants.each do |plant| %>
        <div class="house-page_map-section_item">
          <h3 class="house-page_map-section_item_title"><%= plant.title %></h3>
          <img src="<%= plant.image %>" alt="" class="house-page_map-section_item_image">
        </div>
      <% end %>
    </div>
  </div>
  <div class="house-page_list-section">
    <div class="container">
      <div class="house-page_list-section_list">
        <% empreendimento.more_details.each do |detail| %>
          <div class="house-page_list-section_list_item"><%= detail.title %></div>
        <% end %>
      </div>
    </div>
  </div>

  <% if condominio %>
    <div class="house-page_-condominium-section">
      <div class="house-page_-condominium-section_intro" style="linear-gradient(180deg, rgba(0, 0, 0, 0.55), rgba(0, 0, 0, 0.55)), url('<%= condominio.image %>')">
        <div class="container">
          <div class="house-page_-condominium-section_intro_content">
            <div class="house-page_-condominium-section_intro_content_left">
              <h2 class="house-page_-condominium-section_intro_content_left_title"><%= condominio.title %></h2>
              <div class="house-page_-condominium-section_intro_content_left_subtitle"><%= condominio.city %></div>
              <div class="house-page_-condominium-section_intro_content_p"><%= markdown(condominio.text) %></div>
            </div>
          </div>
        </div>
      </div>
      <div class="house-page_-condominium-section_list-block">
        <div class="container">
          <% condominio.sections.each_with_index do |section, index| %>
            <div class="house-page_about-section_block_item<%= index.even? ? '' : ' invert' %>">
              <div class="house-page_about-section_block_left">
                <h2 class="house-page_-condominium-section_list-block_h2"><%= section.title %></h2>

                <% section.details.each_with_index do |detail, index| %>
                  <div class="house-page_about-section_link">
                    <img src="/images/ck-simbolo.svg" width="20" alt="" class="house-page_about-section_link_icon">
                    <div class="house-page_about-section_link_text"><%= detail.text %></div>

                    <% if index == 0 %>
                      <div class="house-page_about-section_link_line"></div>
                    <% end %>
                  </div>
                <% end %>
              </div>
              <div class="house-page_about-section_block_photo _1" style="background-image: url('<%= section.image %>')"></div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  <% end %>

  <% if empreendimento.show_reasons %>
    <div class="house-page_reasons-section">
      <div class="container">
        <h2 class="house-page_reasons-section_h2"><strong>8 razões</strong> Para Comprar Uma Casa<br>da CK em Gramado na Obra!</h2>
        <div class="house-page_reasons-section_list">
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone1.svg" width="60" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">01. <strong>Economia</strong></h3>
            <p class="house-page_reasons-section_list_item_p">Adquirindo uma de nossas casas durante a obra, você tem uma economia de R$1.000.00,00, simplesmente pelo fato de adquirir antes de estar pronta.</p>
          </div>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone2.svg" width="65" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">02. <strong>Interior e Decoração</strong></h3>
            <p class="house-page_reasons-section_list_item_p">Na compra da casa em construção, Você Ganha os Projetos de Interiores, além de toda orientação de decoração totalmente GRÁTIS!</p>
          </div>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone3.svg" width="69" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">03. <strong>Acompanhe de Perto</strong></h3>
            <p class="house-page_reasons-section_list_item_p">Viaje até três vezes à Gramado durante a construção, com passagens e hospedagem para casal por dois dias por nossa conta, para que possa visitar a obra e participar da escolha de acabamentos se desejar.</p>
          </div>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone6.svg" width="76" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">04. <strong>Aproveite o Condomínio</strong></h3>
            <p class="house-page_reasons-section_list_item_p">A partir da compra, você e sua família já podem usufruir de toda a infraestrutura do condomínio, incluindo piscinas, spa, clube  e todas áreas de lazer.</p>
          </div>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone4.svg" width="70" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">05. <strong>Monitoramento Mensal</strong></h3>
            <p class="house-page_reasons-section_list_item_p">Acompanhe toda a evolução da obra através de fotos e cronograma em nosso site mensalmente.</p>
          </div>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone5.svg" width="70" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">06. <strong>Uma Recordação de Amor</strong></h3>
            <p class="house-page_reasons-section_list_item_p">Você recebe de cortesia, uma escultura exclusiva  do casal ou filhos, feita por um artista plástico.</p>
          </div>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone1.svg" width="60" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">07. <strong>Facilidade de Pagamento</strong></h3>
            <p class="house-page_reasons-section_list_item_p">Adquirindo seu imóvel durante a construção, você tem facilidades especiais de parcelamento, incluindo financiamento imobiliário se preferir.</p>
          </div>
          <div class="house-page_reasons-section_list_item">
            <div class="house-page_reasons-section_list_item_header"><img src="/images/icone7.svg" width="60" alt="" class="house-page_reasons-section_list_item_header_icon"></div>
            <h3 class="house-page_reasons-section_list_item_h3">08. <strong>Valorização</strong></h3>
            <p class="house-page_reasons-section_list_item_p">Ao comprar a casa durante a construção você se beneficia da valorização.</p>
          </div>
        </div>
      </div>
    </div>
  <% end %>
  <div class="house-page_price-section">
    <div class="container">
      <div class="house-page_price-section_list">
        <div class="house-page_price-section_list_item">
          <div class="house-page_price-section_title">Valor da casa</div>
          <div class="house-page_price-section_value"><span class="price-tag">R$</span><%= empreendimento.value %></div>
        </div>
        <div class="house-page_price-section_list_item">
          <div class="house-page_price-section_title">Valor do condomínio</div>
          <div class="house-page_price-section_value"><span class="price-tag">R$</span><%= condominio.value %></div>
        </div>
      </div>
    </div>
  </div>
  <div class="house-page_contact-section">
    <div class="container">
      <h2 class="house-page_contact-section_h2">Vamos <strong class="semi-bold">conversar?</strong></h2>
      <div class="house-page_contact-section_subtitle">Fale diretamente com nossos consultores, a qualquer momento.</div>
      <div class="house-page_contact-section_buttons-box"><a href="https://api.whatsapp.com/send?phone=5555197514953&amp;text=Ol%C3%A1,%20estou%20entrando%20em%20contato%20pelo%20site!" target="_blank" class="button pink w-button">Enviar mensagem pelo WhatsApp</a><a href="mailto:comercial@ckarquitetura.com?subject=Contato%20Site" class="button pink fill margin-left w-button">Entrar em contato pelo e-mail</a></div>
    </div>
  </div>

  <%= partial "partials/footer" %>
  <%= partial "partials/menu-mobile" %>

  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
  <script src="/js/ck-arquitetura.js" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
</body>
</html>
