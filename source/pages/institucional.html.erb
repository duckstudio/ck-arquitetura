    content_for(:page_title, data.pages.institucional.seo.title)
  end
%>
<%
  if data.pages.institucional.seo && data.pages.institucional.seo.description
    content_for(:meta_description, data.pages.institucional.seo.description)
  end
%>
  <div class="fixed-bar">
